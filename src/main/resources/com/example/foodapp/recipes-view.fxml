<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<ScrollPane prefHeight="605.0" prefWidth="913.0" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.foodapp.controller.RecipeController">
   <content>
      <VBox fx:id="recipesLayout" prefHeight="605.0" prefWidth="913.0">
         <children>
            <HBox prefHeight="36.0" prefWidth="476.0">
               <children>
                  <Button fx:id="back" layoutX="25.0" layoutY="26.0" mnemonicParsing="false" onAction="#onBackButtonClick" prefHeight="32.0" prefWidth="78.0" text="Back" textAlignment="CENTER">
                     <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </HBox.margin></Button>
                  <Label fx:id="Name" alignment="CENTER" prefHeight="36.0" prefWidth="138.0" text="Name">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font></Label>
                  <Label fx:id="ingredients" alignment="CENTER" contentDisplay="CENTER" prefHeight="36.0" prefWidth="349.0" text="Ingredients" textOverrun="CLIP">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font></Label>
                  <Label alignment="CENTER" prefHeight="42.0" prefWidth="130.0" text="Calories">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                     <padding>
                        <Insets left="50.0" />
                     </padding>
                  </Label>
                  <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="42.0" prefWidth="194.0" text="Hyperlink">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                     <padding>
                        <Insets right="5.0" />
                     </padding></Label>
               </children>
            </HBox>
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="554.0" prefWidth="914.0" />
         </children>
      </VBox>
   </content>
</ScrollPane>
